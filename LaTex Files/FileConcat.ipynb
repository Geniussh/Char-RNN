   "source": [
    "import glob\n",
    "read_files = glob.glob(\"*.tex\")\n",
    "\n",
    "with open(\"latex.txt\", \"wb\") as outfile:\n",
    "    for f in read_files:\n",
    "        with open(f, \"rb\") as infile:\n",
    "            outfile.write(infile.read())"
   ]
  }
